    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@vitejs/plugin-vue": "^3.0.0",
    "autoprefixer": "^10.4.8",
    "cypress": "^10.4.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.7",
    "typescript": "^4.6.4",
